{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#A9D3AB",
            "style": "plain",
            "template": "\u279c",
            "type": "text"
          },
          {
            "foreground": "#7AABD4",
            "properties": {
              "style": "folder"
            },
            "style": "plain",
            "template": " {{ .Path }} ",
            "type": "path"
          },
          {
            "foreground": "#F58E8E",
            "properties": {
              "branch_icon": ""
            },
            "style": "plain",
            "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>",
            "type": "git"
          },
          {
            "foreground": "#BF616A",
            "style": "plain",
            "template": " \u2717",
            "type": "exit"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }
  